   *"1shoot"*)
     from=$(echo "$res" | perl -pe "s/.*PRIVMSG (.*[#]?([a-zA-Z]|\-)*) :.*/\1/")
     who=$(echo "$res" | perl -pe "s/:(.*)\!.*@.*/\1/")
     gun=$(shuf -n 1 1gun.txt)
     blam=$(echo "blam!!!")
   echo "PRIVMSG $from :ACTION points revolver towards $who" >> $input
   echo "PRIVMSG $from :ACTION $gun" >> $input
   if [ $gun = $blam ]
	then echo " KICK $from $who :you are dead bitch" >> $input
   else echo "meh"
   fi
   ;;
   *"2shoot"*)
     from=$(echo "$res" | perl -pe "s/.*PRIVMSG (.*[#]?([a-zA-Z]|\-)*) :.*/\1/")
